{
  "name": "@bedrock-ui/react-native-ios",
  "version": "0.0.1",
  "description": "Bedrock UI React Native iOS",
  "files": [
    "lib"
  ],
  "module": "lib/esm/index.js",
  "main": "lib/cjs/index.js",
  "types": "lib/cjs/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "pnpm rm:build && pnpm build:cjs && pnpm build:esm",
    "build:cjs": "tsc --project tsconfig.build-cjs.json && tsc-alias -p tsconfig.build-cjs.json",
    "build:esm": "tsc --project tsconfig.build-esm.json && tsc-alias -p tsconfig.build-esm.json",
    "build-storybook": "build-storybook",
    "dev": "start-storybook -p 6006",
    "rm:build": "rimraf lib",
    "storybook": "start-storybook -p 6006",
    "typecheck": "tsc --noEmit"
  },
  "author": "Matthew Wolfe",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.0.21",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.70.1",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "react": "^18",
    "react-native": "^0.70.0"
  }
}
